{
    "name": "OctoEdit",
    "version": "0.4.1",
    "description": "Syntax Highlighting mini-editor for GitHub.com",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["*://github.com/*"],
        "js": ["content-script.js"],
        "run_at": "document_idle"
    }],
    "web_accessible_resources": [
        "pagenav-listener.js"
    ]
}
